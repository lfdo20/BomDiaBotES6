{
  "name": "bomdiacracoviansbot",
  "version": "1.7.0",
  "description": "Bot de Bom dia da craco",
  "main": "index.js",
  "scripts": {
    "postinstall": "babel src -d dist",
    "start": "babel-node ./src/index.js",
    "build": "./node_modules/.bin/webpack --mode production --optimize-minimize",
    "dev": "./node_modules/.bin/webpack --mode development",
    "web": "npm run build && npm run start",
    "go": "npm run postinstall && npm start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lfdo20/BomDiaCracoviansES6.git"
  },
  "keywords": [
    "bot",
    "telegram",
    "craco",
    "bomdia",
    "chatbot"
  ],
  "author": "Leandro Fernandes lfdo <lfdo20@gmail.com> (https://lfdo20.github.io/lfdo)",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "dotenv": "^4.0.0",
    "dropbox": "^2.5.12",
    "express": "^4.16.2",
    "moment": "^2.20.1",
    "node-telegram-bot-api": "^0.29.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "request": "^2.83.0",
    "styled-components": "^3.2.3",
    "twit": "^2.2.9",
    "watson-developer-cloud": "^3.2.1"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  },
  "quokka": {
    "babel": false
  },
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.7.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12"
  }
}
